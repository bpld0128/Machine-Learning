    "# Import Library\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# x = new_group_data['2016']\n",
    "# y = new_group_data['Agency Name']\n",
    "\n",
    "# Plot the data into a bar chart\n",
    "barh_plot = new_group_data.plot(x = 'Agency Name' ,y = '2016' , kind = 'barh', figsize = (10,7), \n",
    "                                fontsize = '10', color = 'grey', \n",
    "                                alpha = 0.5, legend = False)\n",
    "\n",
    "plt.xlabel(None)\n",
    "plt.ylabel(None)\n",
    "plt.title('Congress expenditure in ($) Billions (2016)', color = 'grey')\n",
    "plt.xticks([])\n",
    "plt.yticks(color = 'grey')\n",
    "# ax = plt.gca()\n",
    "\n",
    "# Highlight a bar given by a certain index\n",
    "barh_plot.patches[16].set_color('r')\n",
    "\n",
    "# Remove Borders\n",
    "# plt.xaxis('off')\n",
    "# plt.rcParams['axes.spines.right'] = False\n",
    "# plt.rcParams['axes.spines.top'] = False\n",
    "# plt.rcParams['axes.spines.bottom'] = False\n",
    "# plt.rcParams['axes.spines.left'] = False\n",
    "plt.box(False) #removes the entire box \n",
    "\n",
    "# set individual bar lables using above list\n",
    "for i in barh_plot.patches:\n",
    "    # get_width pulls left or right; get_y pushes up or down\n",
    "    barh_plot.text(i.get_width()+10, i.get_y(), \\\n",
    "            str(round((i.get_width()), 1)), fontsize= 12, color='dimgrey')\n",
    "    "
   ]
